            <Font name="Consolas Bold" size="38.0" />
         </font>
      </Label>
      <TextField alignment="CENTER" focusTraversable="false" layoutX="417.0" layoutY="182.0" opacity="0.76" prefHeight="31.0" prefWidth="223.0" promptText="User Name">
         <font>
            <Font size="13.0" />
         </font>
      </TextField>
      <TextField alignment="CENTER" focusTraversable="false" layoutX="417.0" layoutY="258.0" opacity="0.76" prefHeight="31.0" prefWidth="223.0" promptText="Password">
